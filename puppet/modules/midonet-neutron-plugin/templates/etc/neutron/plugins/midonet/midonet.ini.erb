[DATABASE]
sql_connection = '<%= scope['::neutron:server:database_connection'] %>'
[MIDONET]
# MidoNet API URL
midonet_uri = 'http://'<%= scope['midonet::midonet_api_address'] %>':'<%= scope['midonet::midonet_api_port'] %>'/midonet-api'
# MidoNet administrative user in Keystone
username = <%= scope['midonet::midonet_keystone_username'] %>
password = <%= scope['midonet::midonet_keystone_password'] %>
# MidoNet administrative user's tenant
project_id = <%= scope['midonet::keystone_admin_tenant_name'] %>